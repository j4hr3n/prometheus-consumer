{
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "strict": [0, "global"],
        "class-methods-use-this": [0]
    },
    "overrides": [
        {
            "files": "src/layouts/frontpage/assets/**/*",
            "env": {
                "browser": true
            }
        }
    ]
}
